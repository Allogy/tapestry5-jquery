<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
	<div class="documentation">
		<h3><t:delegate t:to="title" /></h3>
	
		<h4>Description</h4>
		<p><t:delegate t:to="description" /></p>
	
		<t:if test="hasLinks">
			<h4>Links</h4>
			<ul>
				<t:loop t:source="links" t:value="currentLink">
					<li><t:delegate t:to="currentLinkBlock" /></li>
				</t:loop>
			</ul>
		</t:if>
		
		<t:if test="hasParameters">
			<h4>Parameters</h4>
			<table>
				<tr>
					<th>Name</th>
					<th>Required</th>
					<th>Java Type</th>
					<th>Default Prefix</th>
					<th>Default Value</th>
					<th>Description</th>
				</tr>
				<t:loop t:source="params" t:value="currentParam" t:index="index">
					<tr class="${rowClass}"><t:delegate t:to="currentParamBlock" /></tr>
				</t:loop>
			</table>
		</t:if>
	</div>

</t:container>
