        <classes>
            <class name="org.got5.tapestry5.jquery.test.PrototypeTest"></class>
        </classes>

    </test>
   	--> 
    <test name="CarouselTest"  enabled="true">
    
        <parameter name="tapestry.web-app-folder" value="src/test/jquery/webapp"/>
        <parameter name="tapestry.browser-start-command" value="*firefox" />

        <classes>
            <class name="org.got5.tapestry5.jquery.test.CarouselTest"></class>
        </classes>

    </test>

</suite>
