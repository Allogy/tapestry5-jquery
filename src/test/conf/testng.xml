<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="tapestry5-jquery Application Test Suite" annotations="1.5">
   <test name="JQuery Integration Tests"  enabled="true">
    
        <parameter name="tapestry.web-app-folder" value="src/test/jquery/webapp"/>
        <parameter name="tapestry.browser-start-command" value="${browserStartCommand}" />

        <classes>
            <class name="org.got5.tapestry5.jquery.test.JQueryTest"></class>
            <class name="org.got5.tapestry5.jquery.test.CarouselTest"></class>
        </classes>
        
        

    </test>
     
