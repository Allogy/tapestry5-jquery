<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">

	<ul>
		<t:loop source="tabs" value="tab" index="currentPanelId">
            <li><a href="#" t:type="eventLink" t:event="selectTab" t:zone="prop:clientZoneId" context="tabContext">${tabTitle}</a></li>                
        </t:loop>
	</ul>

	<div t:type="zone" t:id="contentZone" id="${clientZoneId}">
		<t:delegate to="ActiveBlock"/>
	</div>	
</t:container>

